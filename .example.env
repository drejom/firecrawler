HOST=0.0.0.0
PORT=3000

# Firecrawl API target URL
FIRECRAWL_API_URL=https://your-firecrawl-api.com

# Optional Firecrawl API key
FIRECRAWL_API_KEY=

# Log level (error, warn, info, debug)
LOG_LEVEL=info

# Node environment (development, production)
NODE_ENV=production

# Set to 'false' to prevent automatically opening the browser on startup
OPEN_BROWSER=true
