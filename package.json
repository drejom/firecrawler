{
  "name": "firecrawler",
  "version": "1.0.0",
  "description": "A lightweight frontend for self-hosted Firecrawl API instances",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 node server.js",
    "dev": "NODE_NO_WARNINGS=1 nodemon server.js",
    "bump": "npx -y standard-version --skip.tag && git add . && git commit -m 'chore: bump version' && git push"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "16.4.7",
    "express": "^4.20.0",
    "http-proxy-middleware": "^3.0.3",
    "open": "10.1.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/http-proxy": "1.17.16",
    "@types/http-proxy-middleware": "0.19.3",
    "@types/node": "22.13.10",
    "eslint": "^8.57.0",
    "jsdoc": "^4.0.2",
    "nodemon": "^3.0.3"
  }
}
